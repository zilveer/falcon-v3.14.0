mixin FeedUserSuggestion(config)
  - var config = Object.assign({start: 0, end: connections.length}, config);
  .card&attributes(attributes)
    .card-header.bg-light.d-flex.justify-content-between.align-items-center
      h5.mb-0 Add to your feed
      a(href='#!').fs--1 See all
    .card-body
      each connection, index in connections.slice(config.start, config.end)
        .d-flex
          +Avatar({img: `team/${connection.img}.jpg`, size: '3xl'})
          .flex-1.ms-2
            h6.mb-0
              a(href=`${CWD}${paths['user-profile']}`)=connection.name
            if(connection.mutual)
              p.fs--1.mb-0 #{connection.mutual} mutual connections
            button.btn.btn-light.btn-sm.py-0.mt-1.border(type='button')
              span.fas.fa-user-plus(data-fa-transform='shrink-5 left-2')
              span.fs--1 Follow
            if(index !== config.end - 1)
              .border-bottom.border-dashed.my-3
